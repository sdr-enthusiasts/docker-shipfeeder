<?xml version="1.0" ?>
<AVNServer>
<AVNConfig loglevel="INFO" debugToLog="true"/>
<Readers>
	<AVNFeeder/>
	<AVNUdpReader port="33333" minTime="100"/>
</Readers>
<!-- provide your data at this port for e.g. OpenCPN -->
<AVNSocketWriter port="34567" maxDevices="5" filter="" read="true" minTime="100" avahiName="avnav-server"/>
<AVNTrackWriter interval="10" distance="100"/>
<AVNHttpServer navurl="/viewer/avnav_navi.php" httpPort="80" upzoom="0" chartbase="maps" empty="nonexistent">
	<Directory urlpath="maps" path="~/AvNavCharts/out"/>
	<MimeType extension=".avt" type="text/plain"/>
	<MimeType extension=".log" type="text/plain"/>
</AVNHttpServer>
<!-- create chart overview files -->
<AVNChartHandler/>
    <!--  routing handler -->
<AVNRouter/>
<!--<AVNNmeaLogger filter="$" interval="0"></AVNNmeaLogger>-->
<AVNNmeaLogger/>
<AVNImporter/>
<AVNCommandHandler>
	<Command name="sound" command="mpg123 -q" repeat="1"/>
</AVNCommandHandler>
<AVNPluginHandler>
